<p id="notice"><%= notice %></p>

<h1>Listing Request Urgencies</h1>

<table>
  <thead>
    <tr>
      <th>Urgency</th>
      <th>Ref</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @request_urgencies.each do |request_urgency| %>
      <tr>
        <td><%= request_urgency.urgency %></td>
        <td><%= request_urgency.ref_id %></td>
        <td><%= link_to 'Show', request_urgency %></td>
        <td><%= link_to 'Edit', edit_request_urgency_path(request_urgency) %></td>
        <td><%= link_to 'Destroy', request_urgency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request urgency', new_request_urgency_path %>

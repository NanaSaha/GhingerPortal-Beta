<p id="notice"><%= notice %></p>

<h1>Listing Service Masters</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Comment</th>
      <th>Entity</th>
      <th>User</th>
      <th>Active status</th>
      <th>Change status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_masters.each do |service_master| %>
      <tr>
        <td><%= service_master.title %></td>
        <td><%= service_master.comment %></td>
        <td><%= service_master.entity_id %></td>
        <td><%= service_master.user_id %></td>
        <td><%= service_master.active_status %></td>
        <td><%= service_master.change_status %></td>
        <td><%= link_to 'Show', service_master %></td>
        <td><%= link_to 'Edit', edit_service_master_path(service_master) %></td>
        <td><%= link_to 'Destroy', service_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service master', new_service_master_path %>

<p id="notice"><%= notice %></p>

<h1>Listing Lab Detail Images</h1>

<table>
  <thead>
    <tr>
      <th>Pre appointment</th>
      <th>Lab detail</th>
      <th>Comment</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lab_detail_images.each do |lab_detail_image| %>
      <tr>
        <td><%= lab_detail_image.pre_appointment_id %></td>
        <td><%= image_tag lab_detail_image.image.url(:medium) %></td>
        <td><%= lab_detail_image.lab_detail_id %></td>
        <td><%= lab_detail_image.comment %></td>
        <td><%= lab_detail_image.user_id %></td>
        <td><%= lab_detail_image.active_status %></td>
        <td><%= lab_detail_image.del_status %></td>
        <td><%= link_to 'Show', lab_detail_image %></td>
        <td><%= link_to 'Edit', edit_lab_detail_image_path(lab_detail_image) %></td>
        <td><%= link_to 'Destroy', lab_detail_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lab detail image', new_lab_detail_image_path %>

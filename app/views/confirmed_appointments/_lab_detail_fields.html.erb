<!--<div class="col-md-1"></div>-->
<div class="pre-scrollable" style="max-height: 200px">
  <!--<div  class="col-md-10">-->
    <div class="nested-fields" >
      <h4>Select lab service(s)</h4>
      <% count = 1 %>
      <% unless  @lab_services.nil? %>

          <% @lab_services.each do |lab_service| %>
              <div class="table-responsive">
                <table class="table table-condensed table-bordered w3-striped">
                  <tr class= "w3-teal">
                    <td style="width: 5%"><%= "#{count}. " %></td>
                    <td style="width: 70%; " class="w3-white">&nbsp;<%= "#{lab_service.lab_service.title}" %></td>
                    <td style="width: 15%; " class="w3-white">&nbsp;<%= "#{lab_service.fee}" %></td>
                    <%#= @lab_details.inspect %>
                    <td class="w3-white">

                      <% if @lab_details.include?(lab_service.lab_service.id) %>
                          <input type="checkbox" name="lab_service_id[<%=count%>[checkbox]]" checked="checked" />
                      <% else %>
                          <input type="checkbox" name="lab_service_id[<%=count%>[checkbox]]"/>
                      <% end %>
                      <input type="hidden" name="lab_service_id[<%=count%>[service_id]]"  value="<%= lab_service.service_id %>" required>
                      <% count += 1 %>
                    </td>

                  </tr>

                </table>
              </div>
          <% end %>
      <% end %>

    </div>
  <!--</div>-->
</div>

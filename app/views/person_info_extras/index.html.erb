<p id="notice"><%= notice %></p>

<h1>Listing Person Info Extras</h1>

<table>
  <thead>
    <tr>
      <th>Person</th>
      <th>Specialty</th>
      <th>Specialty duration</th>
      <th>Has specialty</th>
      <th>Forign training</th>
      <th>Foreign institution desc</th>
      <th>Profession group</th>
      <th>Licence number</th>
      <th>Comment</th>
      <th>User</th>
      <th>Entity</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @person_info_extras.each do |person_info_extra| %>
      <tr>
        <td><%= person_info_extra.person_id %></td>
        <td><%= person_info_extra.specialty_id %></td>
        <td><%= person_info_extra.specialty_duration %></td>
        <td><%= person_info_extra.has_specialty %></td>
        <td><%= person_info_extra.forign_training %></td>
        <td><%= person_info_extra.foreign_institution_desc %></td>
        <td><%= person_info_extra.profession_group_id %></td>
        <td><%= person_info_extra.licence_number %></td>
        <td><%= person_info_extra.comment %></td>
        <td><%= person_info_extra.user_id %></td>
        <td><%= person_info_extra.entity_id %></td>
        <td><%= person_info_extra.active_status %></td>
        <td><%= person_info_extra.del_status %></td>
        <td><%= link_to 'Show', person_info_extra %></td>
        <td><%= link_to 'Edit', edit_person_info_extra_path(person_info_extra) %></td>
        <td><%= link_to 'Destroy', person_info_extra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p id="notice"><%= notice %></p>

<h1>Listing Service Prov Extra Infos</h1>

<table>
  <thead>
    <tr>
      <th>Service prov</th>
      <th>Suburb</th>
      <th>Contact number1</th>
      <th>Contact number2</th>
      <th>Postal address</th>
      <th>Location address</th>
      <th>Contact person name</th>
      <th>Web url</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Comment</th>
      <th>Entity</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_prov_extra_infos.each do |service_prov_extra_info| %>
      <tr>
        <td><%= service_prov_extra_info.service_prov_id %></td>
        <td><%= service_prov_extra_info.suburb_id %></td>
        <td><%= service_prov_extra_info.contact_number1 %></td>
        <td><%= service_prov_extra_info.contact_number2 %></td>
        <td><%= service_prov_extra_info.postal_address %></td>
        <td><%= service_prov_extra_info.location_address %></td>
        <td><%= service_prov_extra_info.contact_person_name %></td>
        <td><%= service_prov_extra_info.web_url %></td>
        <td><%= service_prov_extra_info.longitude %></td>
        <td><%= service_prov_extra_info.latitude %></td>
        <td><%= service_prov_extra_info.comment %></td>
        <td><%= service_prov_extra_info.entity_id %></td>
        <td><%= service_prov_extra_info.user_id %></td>
        <td><%= service_prov_extra_info.active_status %></td>
        <td><%= service_prov_extra_info.del_status %></td>
        <td><%= link_to 'Show', service_prov_extra_info, remote: true %></td>
        <td><%= link_to 'Edit', edit_service_prov_extra_info_path(service_prov_extra_info) %></td>
        <td><%= link_to 'Destroy', service_prov_extra_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


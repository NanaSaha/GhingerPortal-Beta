<%= simple_form_for(@payment_req) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :billing_info_id %>
    <%= f.input :amount %>
    <%= f.input :reference %>
    <%= f.input :exttrid %>
    <%= f.input :mobile_number %>
    <%= f.input :nw %>
    <%= f.input :callback_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

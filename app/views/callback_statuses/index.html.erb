<p id="notice"><%= notice %></p>

<h1>Listing Callback Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Trans status</th>
      <th>Trans</th>
      <th>Exttrid</th>
      <th>Trans msg</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @callback_statuses.each do |callback_status| %>
      <tr>
        <td><%= callback_status.trans_status %></td>
        <td><%= callback_status.trans_id %></td>
        <td><%= callback_status.exttrid %></td>
        <td><%= callback_status.trans_msg %></td>
        <td><%= link_to 'Show', callback_status %></td>
        <td><%= link_to 'Edit', edit_callback_status_path(callback_status) %></td>
        <td><%= link_to 'Destroy', callback_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Callback status', new_callback_status_path %>

<p id="notice"><%= notice %></p>

<h1>Listing Provider Masters</h1>

<table>
  <thead>
    <tr>
      <th>Provider name</th>
      <th>Alias</th>
      <th>Provider type</th>
      <th>Comment</th>
      <th>Entity</th>
      <th>User</th>
      <th>Active status</th>
      <th>Change status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @provider_masters.each do |provider_master| %>
      <tr>
        <td><%= provider_master.provider_name %></td>
        <td><%= provider_master.alias %></td>
        <td><%= provider_master.provider_type_id %></td>
        <td><%= provider_master.comment %></td>
        <td><%= provider_master.entity_id %></td>
        <td><%= provider_master.user_id %></td>
        <td><%= provider_master.active_status %></td>
        <td><%= provider_master.change_status %></td>
        <td><%= link_to 'Show', provider_master %></td>
        <td><%= link_to 'Edit', edit_provider_master_path(provider_master) %></td>
        <td><%= link_to 'Destroy', provider_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Provider master', new_provider_master_path %>

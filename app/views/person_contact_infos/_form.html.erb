<%= simple_form_for(@person_contact_info) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :person_id %>
    <%= f.input :contact_number %>
    <%= f.input :email %>
    <%= f.input :postal_address %>
    <%= f.input :location_address %>
    <%= f.input :comment %>
    <%= f.input :user_id %>
    <%= f.input :entity_id %>
    <%= f.input :active_status %>
    <%= f.input :del_status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

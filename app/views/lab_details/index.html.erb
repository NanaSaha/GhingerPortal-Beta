<p id="notice"><%= notice %></p>

<h1>Listing Lab Details</h1>

<table>
  <thead>
    <tr>
      <th>Pre appointment</th>
      <th>Lab service</th>
      <th>Comment</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lab_details.each do |lab_detail| %>
      <tr>
        <td><%= lab_detail.pre_appointment_id %></td>
        <td><%= lab_detail.lab_service_id %></td>
        <td><%= lab_detail.comment %></td>
        <td><%= lab_detail.user_id %></td>
        <td><%= lab_detail.active_status %></td>
        <td><%= lab_detail.del_status %></td>
        <td><%= link_to 'Show', lab_detail %></td>
        <td><%= link_to 'Edit', edit_lab_detail_path(lab_detail) %></td>
        <td><%= link_to 'Destroy', lab_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lab detail', new_lab_detail_path %>

<%= simple_form_for(@country_master, :remote => true ) do |f| %>
  
  <%= f.error_notification %>
  
  <div class="form-inputs">
    <%= f.input :country_name%>
    <%= f.input :comment %>
    <%= f.input :entity_id, as: :hidden %>
    <%= f.input :user_id, as: :hidden, input_html: {value: current_user.id} %>
    <%= f.input :active_status, as: :hidden, input_html: {value: 1} %>
    <%= f.input :del_status, as: :hidden, input_html: {value: 0} %>
  </div>

  <div class="form-actions">
  	<%= link_to 'Back', :back, class: "w3-left w3-button w3-small w3-round-large w3-blue w3-hover-blue" %>
    <%= f.button :submit, class: "w3-right w3-button w3-small w3-round-large w3-teal w3-hover-teal", value: "Update Country" %>
    <br />
  </div> 
<% end %>
<br />

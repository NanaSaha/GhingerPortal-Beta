<p id="notice"><%= notice %></p>

<h1>Listing Subscription Remarks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subscription_remarks.each do |subscription_remark| %>
      <tr>
        <td><%= subscription_remark.name %></td>
        <td><%= link_to 'Show', subscription_remark %></td>
        <td><%= link_to 'Edit', edit_subscription_remark_path(subscription_remark) %></td>
        <td><%= link_to 'Destroy', subscription_remark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subscription remark', new_subscription_remark_path %>

<p id="notice"><%= notice %></p>

<h1>Listing Engaged User Services</h1>

<table>
  <thead>
    <tr>
      <th>Person</th>
      <th>User service</th>
      <th>Comment</th>
      <th>Entity</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @engaged_user_services.each do |engaged_user_service| %>
      <tr>
        <td><%= engaged_user_service.person_id %></td>
        <td><%= engaged_user_service.user_service_id %></td>
        <td><%= engaged_user_service.comment %></td>
        <td><%= engaged_user_service.entity_id %></td>
        <td><%= engaged_user_service.user_id %></td>
        <td><%= engaged_user_service.active_status %></td>
        <td><%= engaged_user_service.del_status %></td>
        <td><%= link_to 'Show', engaged_user_service %></td>
        <td><%= link_to 'Edit', edit_engaged_user_service_path(engaged_user_service) %></td>
        <td><%= link_to 'Destroy', engaged_user_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Engaged user service', new_engaged_user_service_path %>

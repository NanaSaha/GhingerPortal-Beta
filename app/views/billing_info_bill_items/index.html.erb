<p id="notice"><%= notice %></p>

<h1>Listing Billing Info Bill Items</h1>

<table>
  <thead>
    <tr>
      <th>Billing info</th>
      <th>Item</th>
      <th>Item price</th>
      <th>Item price</th>
      <th>Quantity</th>
      <th>Comment</th>
      <th>User</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @billing_info_bill_items.each do |billing_info_bill_item| %>
      <tr>
        <td><%= billing_info_bill_item.billing_info_id %></td>
        <td><%= billing_info_bill_item.item_id %></td>
        <td><%= billing_info_bill_item.item_price %></td>
        <td><%= billing_info_bill_item.item_price %></td>
        <td><%= billing_info_bill_item.quantity %></td>
        <td><%= billing_info_bill_item.comment %></td>
        <td><%= billing_info_bill_item.user_id %></td>
        <td><%= billing_info_bill_item.active_status %></td>
        <td><%= billing_info_bill_item.del_status %></td>
        <td><%= link_to 'Show', billing_info_bill_item %></td>
        <td><%= link_to 'Edit', edit_billing_info_bill_item_path(billing_info_bill_item) %></td>
        <td><%= link_to 'Destroy', billing_info_bill_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


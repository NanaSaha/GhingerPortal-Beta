<p id="notice"><%= notice %></p>

<h1>Listing Provider Engaged Users</h1>

<table>
  <thead>
    <tr>
      <th>Provider</th>
      <th>Person</th>
      <th>Comment</th>
      <th>User</th>
      <th>Entity</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @provider_engaged_users.each do |provider_engaged_user| %>
      <tr>
        <td><%= provider_engaged_user.provider_id %></td>
        <td><%= provider_engaged_user.person_id %></td>
        <td><%= provider_engaged_user.comment %></td>
        <td><%= provider_engaged_user.user_id %></td>
        <td><%= provider_engaged_user.entity_id %></td>
        <td><%= provider_engaged_user.active_status %></td>
        <td><%= provider_engaged_user.del_status %></td>
        <td><%= link_to 'Show', provider_engaged_user %></td>
        <td><%= link_to 'Edit', edit_provider_engaged_user_path(provider_engaged_user) %></td>
        <td><%= link_to 'Destroy', provider_engaged_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Provider engaged user', new_provider_engaged_user_path %>

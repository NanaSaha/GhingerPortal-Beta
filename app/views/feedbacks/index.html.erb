<h4>Feedbacks</h4>
<div class="table-responsive">
  <table class="table table-condensed table-bordered table-striped w3-card">
    <thead>
      <tr class="w3-teal">
        <th>Requester</th>
        <th>Requester name</th>
        <th>Appointment type</th>
        <th>Message</th>
        <!-- <th colspan="3"></th> -->
      </tr>
    </thead>
    <tbody>
      <% @feedbacks.each do |feedback| %>
        <tr>
          <td><%= feedback.requester_id %></td>
          <td><%= feedback.requester_name %></td>
          <td><%= feedback.appointment_type %></td>
          <td><%= feedback.message %></td>
          <td><%#= link_to 'Show', feedback %></td>
          <td><%#= link_to 'Edit', edit_feedback_path(feedback) %></td>
          <td><%#= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<br>
<%#= link_to 'New Feedback', new_feedback_path %>
